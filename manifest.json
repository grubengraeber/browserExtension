{
    "manifest_version": 3,
    "name": "Clockodo-Outlook",
    "version": "0.0.1",
    "description": "A synchronisation tool for clockodo and outlook events",
    "action": {
        "default_icon": {
            "16": "icon.png",
            "24": "icon.png",
            "32": "icon.png"
        },
        "default_title": "Synchodolook",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "options_page": "options.html",
    "permissions": [
        "storage"
    ]
  }